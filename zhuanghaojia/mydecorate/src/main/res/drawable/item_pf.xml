<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!--智能报价评分按钮用到的-->
    <item android:color="@color/blue" android:state_pressed="true" />
    <item android:color="@color/text_gray" android:state_pressed="false" />
    <item android:color="@color/blue" android:state_selected="true" />
    <item android:color="@color/text_gray" android:state_selected="false" />
    <item android:color="@color/blue" android:state_checked="true" />
    <item android:color="@color/text_gray" android:state_checked="false" />
</selector>