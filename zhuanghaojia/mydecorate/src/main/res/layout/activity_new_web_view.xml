<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.tobosu.mydecorate.activity.NewWebViewActivity">
    <RelativeLayout
        android:id="@+id/new_web_view_rl"
        android:layout_width="match_parent"
        android:layout_height="49dp" >
        <RelativeLayout
            android:id="@+id/iv_back_webview_activity_rl"
            android:layout_alignParentLeft="true"
            android:layout_width="49dp"
            android:layout_height="49dp">
            <ImageView
                android:id="@+id/iv_back_webview_activity"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_centerInParent="true"
                android:background="@mipmap/activity_back" />
        </RelativeLayout>


        <TextView
            android:id="@+id/web_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="30dp"
            android:layout_marginRight="10dp"
            android:ellipsize="end"
            android:layout_centerInParent="true"
            android:gravity="center_horizontal"
            android:ems="12"
            android:maxLines="1"
            android:textSize="18sp" />

    </RelativeLayout>

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/dividing_line_bg" />

    <WebView
        android:id="@+id/my_webview"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />
</LinearLayout>
