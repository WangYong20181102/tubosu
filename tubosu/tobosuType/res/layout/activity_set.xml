<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/mainLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:fitsSystemWindows="true"
    android:orientation="vertical" >


	<LinearLayout
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:orientation="vertical">

		<RelativeLayout
			android:id="@+id/rel_head_setting"
			android:layout_width="match_parent"
			android:layout_height="50dp"
			android:background="@color/color_icon" >

			<ImageView
				android:id="@+id/myset_back"
				android:layout_width="30dp"
				android:layout_height="30dp"
				android:layout_centerVertical="true"
				android:padding="5dp"
				android:src="@drawable/back_up" />

			<TextView
				android:id="@+id/myset_title"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_centerHorizontal="true"
				android:layout_centerVertical="true"
				android:text="@string/my_set"
				android:textColor="@color/white"
				android:textSize="16sp" />
		</RelativeLayout>

		<LinearLayout
			android:id="@+id/ll_logo"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:background="#F0F0F0"
			android:layout_below="@+id/rel_head_setting"
			android:gravity="center"
			android:orientation="vertical" >

			<ImageView
				android:id="@+id/myset_logo"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:padding="10dp"
				android:src="@drawable/myset_logo" />

			<TextView
				android:id="@+id/myset_logo_name"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:padding="8dp"
				android:text="tuboshu v3.1"
				android:textSize="15sp" />
		</LinearLayout>

		<ImageView
			android:layout_width="match_parent"
			android:layout_height="1dp"
			android:layout_below="@+id/ll_logo"
			android:background="@color/bg_gray" />

		<RelativeLayout
			android:id="@+id/rel_feedback"
			android:layout_width="match_parent"
			android:layout_height="47dp"
			android:background="@drawable/userinfoselector" >

			<TextView
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginLeft="14dp"
				android:layout_centerVertical="true"
				android:padding="10dp"
				android:text="反馈与建议"
				android:textSize="15sp" />

			<ImageView
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_alignParentRight="true"
				android:layout_centerVertical="true"
				android:layout_marginRight="20dp"
				android:src="@drawable/right" />
		</RelativeLayout>

		<ImageView
			android:layout_width="match_parent"
			android:layout_height="1dp"
			android:layout_marginLeft="10dp"
			android:background="@color/bg_gray" />


		<RelativeLayout
			android:id="@+id/rel_clear_cache"
			android:layout_width="match_parent"
			android:layout_height="47dp"
			android:background="@drawable/userinfoselector" >

			<TextView
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginLeft="14dp"
				android:layout_centerVertical="true"
				android:padding="10dp"
				android:text="@string/myset_clear_cache"
				android:textSize="15sp" />

			<TextView
				android:id="@+id/tv_cache"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_alignParentRight="true"
				android:layout_centerVertical="true"
				android:layout_marginRight="40dp"
				android:padding="10dp"
				android:text="110 M"
				android:textSize="15sp" />

			<ImageView
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_alignParentRight="true"
				android:layout_centerVertical="true"
				android:layout_marginRight="20dp"
				android:src="@drawable/right" />
		</RelativeLayout>

		<ImageView
			android:layout_width="match_parent"
			android:layout_height="1dp"
			android:layout_marginLeft="10dp"
			android:background="@color/bg_gray" />

		<RelativeLayout
			android:id="@+id/rel_push"
			android:layout_width="match_parent"
			android:layout_height="47dp"
			android:background="@drawable/userinfoselector" >

			<TextView
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginLeft="14dp"
				android:layout_centerVertical="true"
				android:padding="10dp"
				android:text="@string/myset_push_message"
				android:textSize="15sp" />

			<ImageView
				android:id="@+id/iv_push_flag"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_alignParentRight="true"
				android:background="@drawable/icon_push_no"
				android:layout_centerVertical="true"
				android:layout_marginRight="20dp" />
		</RelativeLayout>

		<ImageView
			android:layout_width="match_parent"
			android:layout_height="1dp"
			android:layout_marginLeft="10dp"
			android:background="@color/bg_gray" />

		<RelativeLayout
			android:id="@+id/myset_layout_version_upgrade"
			android:layout_width="match_parent"
			android:layout_height="47dp"
			android:visibility="gone"
			android:background="@drawable/userinfoselector" >

			<TextView
				android:id="@+id/myset_version_upgrade_name"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_alignParentLeft="true"
				android:layout_alignParentTop="true"
				android:padding="10dp"
				android:text="@string/myset_version_upgrade"
				android:textSize="15sp" />

			<TextView
				android:id="@+id/myset_version_upgrade"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_alignParentRight="true"
				android:layout_centerVertical="true"
				android:layout_marginRight="40dp"
				android:padding="10dp"
				android:textColor="@color/color_icon"
				android:textSize="15sp" />

			<ImageView
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_alignParentRight="true"
				android:layout_centerVertical="true"
				android:layout_marginRight="20dp"
				android:src="@drawable/right" />
		</RelativeLayout>

		<View
			android:id="@+id/v_line4"
			android:layout_width="match_parent"
			android:layout_height="10dp"
			android:background="@color/bg_gray" />

		<RelativeLayout
			android:id="@+id/rel_company_tel"
			android:layout_width="match_parent"
			android:layout_height="47dp"
			android:background="@drawable/userinfoselector" >

			<TextView
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginLeft="14dp"
				android:layout_centerVertical="true"
				android:padding="10dp"
				android:text="土拨鼠客服"
				android:textSize="15sp" />

			<TextView
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_alignParentRight="true"
				android:layout_centerVertical="true"
				android:layout_marginRight="23dp"
				android:gravity="center"
				android:text="@string/myset_service_tels"
				android:textColor="#333333"
				android:textSize="15sp"
				android:textStyle="bold" />

		</RelativeLayout>

		<ImageView
			android:layout_width="match_parent"
			android:layout_height="1dp"
			android:layout_marginLeft="10dp"
			android:background="@color/bg_gray" />


		<RelativeLayout
			android:id="@+id/myset_layout_tbs_weixin"
			android:layout_width="match_parent"
			android:layout_height="47dp"
			android:background="@drawable/userinfoselector" >

			<TextView
				android:id="@+id/myset_tbs_weixin_name"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginLeft="14dp"
				android:layout_centerVertical="true"
				android:padding="10dp"
				android:text="@string/myset_tbs_weixin"
				android:textSize="15sp" />

			<TextView
				android:id="@+id/myset_tbs_weixin"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_alignParentRight="true"
				android:layout_centerVertical="true"
				android:layout_marginRight="40dp"
				android:padding="10dp"
				android:text="itobosu" />

			<ImageView
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_alignParentRight="true"
				android:layout_centerVertical="true"
				android:layout_marginRight="20dp"
				android:src="@drawable/right" />
		</RelativeLayout>

		<ImageView
			android:layout_width="match_parent"
			android:layout_height="1dp"
			android:background="@color/bg_gray" />

		<RelativeLayout
			android:id="@+id/rel_layout_share"
			android:layout_width="match_parent"
			android:layout_height="47dp"
			android:background="@drawable/userinfoselector" >

			<TextView
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginLeft="14dp"
				android:layout_centerVertical="true"
				android:padding="10dp"
				android:text="@string/myset_share"
				android:textSize="15sp" />

			<ImageView
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_alignParentRight="true"
				android:layout_centerVertical="true"
				android:layout_marginRight="20dp"
				android:src="@drawable/right" />
		</RelativeLayout>

		<ImageView
			android:layout_width="match_parent"
			android:layout_height="1dp"
			android:background="@color/bg_gray" />


		<RelativeLayout
			android:id="@+id/rel_good_praise"
			android:layout_width="match_parent"
			android:layout_height="47dp"
			android:background="@drawable/userinfoselector" >

			<TextView
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginLeft="14dp"
				android:layout_centerVertical="true"
				android:padding="10dp"
				android:text="@string/myset_goodraise"
				android:textSize="15sp" />

			<ImageView
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_alignParentRight="true"
				android:layout_centerVertical="true"
				android:layout_marginRight="20dp"
				android:src="@drawable/right" />
		</RelativeLayout>

		<ImageView
			android:layout_width="match_parent"
			android:layout_height="1dp"
			android:layout_marginLeft="10dp"
			android:background="@color/bg_gray" />

		<RelativeLayout
			android:id="@+id/myset_layout_service_tel"
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			android:background="#E4E4E4">

			<RelativeLayout
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_centerInParent="true" >

				<TextView
					android:id="@+id/myset_foot_company"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_above="@+id/myset_foot_url"
					android:layout_centerHorizontal="true"
					android:gravity="bottom"
					android:text="@string/myset_foot_company" />

				<TextView
					android:id="@+id/myset_foot_url"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_centerHorizontal="true"
					android:layout_alignParentBottom="true"
					android:gravity="bottom"
					android:layout_marginBottom="12dp"
					android:padding="2dp"
					android:text="@string/myset_foot_url" />
			</RelativeLayout>
		</RelativeLayout>

	</LinearLayout>

</LinearLayout>