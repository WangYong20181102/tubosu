<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/mainLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#E4E4E4"
    android:fitsSystemWindows="true"
    android:orientation="vertical" >


        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <RelativeLayout
                android:id="@+id/rel_head_setting"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:background="@color/color_icon" >

                <ImageView
                    android:id="@+id/myset_back"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_centerVertical="true"
                    android:padding="5dp"
                    android:src="@drawable/back_up" />

                <TextView
                    android:id="@+id/myset_title"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:layout_centerVertical="true"
                    android:text="@string/my_set"
                    android:textColor="@color/white"
                    android:textSize="14sp" />
            </RelativeLayout>

            <LinearLayout
                android:id="@+id/ll_logo"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/white"
                android:layout_below="@+id/rel_head_setting"
                android:gravity="center"
                android:orientation="vertical" >

                <ImageView
                    android:id="@+id/myset_logo"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="10dp"
                    android:src="@drawable/myset_logo" />

                <TextView
                    android:id="@+id/myset_logo_name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="8dp"
                    android:textSize="14sp" />
            </LinearLayout>

            <ImageView
                android:id="@+id/iv_line_1"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_below="@+id/ll_logo"
                android:background="@color/bg_gray" />

            <RelativeLayout
                android:id="@+id/rel_clear_cache"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/iv_line_1"
                android:background="@drawable/userinfoselector" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentTop="true"
                    android:padding="10dp"
                    android:text="@string/myset_clear_cache"
                    android:textSize="14sp" />

                <TextView
                    android:id="@+id/tv_cache"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="40dp"
                    android:padding="10dp"
                    android:text="110 M"
                    android:textSize="14sp" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="20dp"
                    android:src="@drawable/right" />
            </RelativeLayout>

            <ImageView
                android:id="@+id/iv_line_2"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginLeft="10dp"
                android:layout_below="@+id/rel_clear_cache"
                android:background="@color/bg_gray" />

            <RelativeLayout
                android:id="@+id/rel_push"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/iv_line_2"
                android:background="@drawable/userinfoselector" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentTop="true"
                    android:layout_centerVertical="true"
                    android:padding="10dp"
                    android:text="@string/myset_push_message"
                    android:textSize="14sp" />

                <ImageView
                    android:id="@+id/iv_push_flag"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="20dp" />
            </RelativeLayout>

            <ImageView
                android:id="@+id/iv_line_3"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginLeft="10dp"
                android:layout_below="@+id/rel_push"
                android:background="@color/bg_gray" />

            <RelativeLayout
                android:id="@+id/myset_layout_version_upgrade"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
				android:visibility="gone"
                android:layout_below="@+id/iv_line_3"
                android:background="@drawable/userinfoselector" >

                <TextView
                    android:id="@+id/myset_version_upgrade_name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentTop="true"
                    android:padding="10dp"
                    android:text="@string/myset_version_upgrade"
                    android:textSize="14sp" />

                <TextView
                    android:id="@+id/myset_version_upgrade"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="40dp"
                    android:padding="10dp"
                    android:textColor="@color/color_icon"
                    android:textSize="14sp" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="20dp"
                    android:src="@drawable/right" />
            </RelativeLayout>

            <View
                android:id="@+id/v_line4"
                android:layout_width="match_parent"
                android:layout_height="10dp"
                android:layout_below="@+id/myset_layout_version_upgrade"
                android:background="@color/bg_gray" />

            <RelativeLayout
                android:id="@+id/rel_good_praise"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/v_line4"
                android:background="@drawable/userinfoselector" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentTop="true"
                    android:padding="10dp"
                    android:text="@string/myset_goodraise"
                    android:textSize="14sp" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="20dp"
                    android:src="@drawable/right" />
            </RelativeLayout>

            <ImageView
                android:id="@+id/iv_5"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginLeft="10dp"
                android:layout_below="@+id/rel_good_praise"
                android:background="@color/bg_gray" />

            <RelativeLayout
                android:id="@+id/myset_layout_tbs_weixin"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/iv_5"
                android:background="@drawable/userinfoselector" >

                <TextView
                    android:id="@+id/myset_tbs_weixin_name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentTop="true"
                    android:padding="10dp"
                    android:text="@string/myset_tbs_weixin"
                    android:textSize="14sp" />

                <TextView
                    android:id="@+id/myset_tbs_weixin"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="40dp"
                    android:padding="10dp"
                    android:text="itobosu" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="20dp"
                    android:src="@drawable/right" />
            </RelativeLayout>

            <ImageView
                android:id="@+id/iv_7"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_below="@+id/myset_layout_tbs_weixin"
                android:background="@color/bg_gray" />

            <RelativeLayout
                android:id="@+id/rel_layout_share"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/iv_7"
                android:background="@drawable/userinfoselector" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentTop="true"
                    android:padding="10dp"
                    android:text="@string/myset_share"
                    android:textSize="14sp" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="20dp"
                    android:src="@drawable/right" />
            </RelativeLayout>

            <ImageView
                android:id="@+id/iv_8"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_below="@+id/rel_layout_share"
                android:background="@color/bg_gray" />

	        <RelativeLayout
	            android:id="@+id/myset_layout_service_tel"
	            android:layout_width="match_parent"
	            android:layout_height="fill_parent"
	            android:layout_below="@+id/iv_8" >
	
	            <TextView
	                android:id="@+id/tv_company_service_tel"
	                android:layout_width="wrap_content"
	                android:layout_height="40dip"
	                android:layout_marginTop="26dip"
	                android:layout_centerHorizontal="true"
	                android:background="@drawable/textview_bg_service"
	                android:gravity="center"
	                android:paddingLeft="30dip"
	                android:paddingRight="30dip"
	                android:text="@string/myset_service_tels"
	                android:textColor="#333333"
	                android:textSize="14sp"
	                android:textStyle="bold" />
				
	            <RelativeLayout
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:layout_alignParentBottom="true"
		            android:layout_marginBottom="12dip"
		            android:layout_centerHorizontal="true" >
		                
		            <TextView
		                android:id="@+id/myset_foot_company"
		                android:layout_width="wrap_content"
		                android:layout_height="wrap_content"
		                android:layout_above="@+id/myset_foot_url"
		                android:layout_centerHorizontal="true"
		                android:gravity="bottom"
		                android:text="@string/myset_foot_company" />
		
		            <TextView
		                android:id="@+id/myset_foot_url"
		                android:layout_width="wrap_content"
		                android:layout_height="wrap_content"
		                android:layout_centerHorizontal="true"
		                android:layout_alignParentBottom="true"
		                android:gravity="bottom"
		                android:layout_marginBottom="12dp"
		                android:padding="2dp"
		                android:text="@string/myset_foot_url" />
		            </RelativeLayout>
	        </RelativeLayout>
               
        </RelativeLayout>

</LinearLayout>